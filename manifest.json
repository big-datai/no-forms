{
    "manifest_version": 3,
    "name": "No Forms",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["popup.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  